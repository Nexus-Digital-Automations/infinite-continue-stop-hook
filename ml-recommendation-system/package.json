{
  "name": "ml-recommendation-engine",
  "version": "1.0.0",
  "description": "Machine Learning Recommendation Engine using TensorFlow.js and Neural Networks",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles --forceExit",
    "lint": "eslint src tests --ext .js,.json",
    "lint:fix": "eslint src tests --ext .js,.json --fix",
    "build": "node scripts/build-models.js",
    "train": "node src/training/train-model.js",
    "validate": "npm run lint && npm run test && npm run build",
    "benchmark": "node benchmarks/performance-test.js",
    "docs": "jsdoc src -r -d docs"
  },
  "keywords": [
    "machine-learning",
    "recommendations", 
    "neural-networks",
    "tensorflow",
    "ai",
    "nodejs"
  ],
  "author": "Claude Code ML Engine",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs": "^4.10.0",
    "@tensorflow/tfjs-node": "^4.10.0",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "node-cache": "^5.1.2",
    "redis": "^4.6.7",
    "csv-parser": "^3.0.0",
    "json2csv": "^6.1.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "dotenv": "^16.3.1",
    "cluster": "^0.7.7",
    "os": "^0.1.2",
    "axios": "^1.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "jest": "^29.6.1",
    "supertest": "^6.3.3",
    "eslint": "^8.44.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "jsdoc": "^4.0.2",
    "@types/node": "^20.4.2",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "bugs": {
    "url": "."
  },
  "homepage": "."
}